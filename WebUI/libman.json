{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@7.0.1",
      "destination": "wwwroot/font-awesome/"
    },
    {
      "library": "Chart.js@4.4.0",
      "destination": "wwwroot/lib/chart.js/",
      "files": [
        "chart.umd.min.js",
        "chart.umd.js"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr/",
      "files": [
        "toastr.min.css",
        "toastr.min.js"
      ]
    }
  ]
}